{  
  "ConnectionString": "Server=tcp:127.0.0.1,5433;Initial Catalog=Microsoft.eShopOnContainers.Services.IdentityDB;User Id=sa;Password=Pass@word",
  "IsClusterEnv": "False",
  "MvcClient": "http://localhost:5100",
  "SpaClient": "http://localhost:5104",
  "XamarinCallback": "http://localhost:5105/xamarincallback",
  "UseCustomizationData": false,
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "UseVault": false,
  "Vault": {
    "Name": "eshop",
    "ClientId": "your-clien-id",
    "ClientSecret": "your-client-secret"
  },
  "spring": {
    "application": {
      "name":  "peterszone-identity-api"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl":  "http://localhost:8761/eureka/",      
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": false,
      "ValidateCertificates":  false
    },
    "instance": {
      "hostname": "localhost",
      "port": 5105,
      "statusPageUrlPath": "/cloudfoundryapplication/info",
      "healthCheckUrlPath": "/cloudfoundryapplication/health"
    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication"
    }
  }
}
